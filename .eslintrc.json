{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "mongo": true
  },
  "extends": ["standard", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "handle-callback-err": 0,
    "space-before-function-paren": "off",
    "import/first": "off"
  }
}
